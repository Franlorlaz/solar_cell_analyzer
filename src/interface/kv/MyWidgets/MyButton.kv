<MyButton@Button>:
    id: BB
    color: .2, .2, .2
    background_normal: ''
    background_color: 0.7, 0.7, 0.7
    background_down: ''
    on_press: BB.background_color = utils.get_color_from_hex('#22BCF5')  # Press state color
    on_release: BB.background_color = 0.7, 0.7, 0.7  # Normal state color
    font_size: 15
